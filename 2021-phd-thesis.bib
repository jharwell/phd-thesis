keywords = {Exploration,Fuzzy control,Multi-robot system,Search and rescue,Swarm robotics},
pages = {4--9},
publisher = {IEEE},
title = {{Fuzzy manual control of multi-robot system with built-in swarm behavior}},
year = {2009}
