isbn = {9781450351669},
journal = {Proceedings of the 14th ACM Symposium on Performance Evaluation of Wireless Ad Hoc, Sensor, {\&} Ubiquitous Networks  - PE-WASUN '17},
pages = {41--45},
title = {{Control of a Remote Swarm of Drones/Robots Through a Local (Possibly Model) Swarm}},
nourl = {http://dl.acm.org/citation.cfm?doid=3134829.3134840},
