@article{Quijano2007,
abstract = {A model of honey bee social foraging is introduced to create an algorithm that solves a class of optimal resource allocation problems. We prove that if several such algorithms compete in the same problem domain, the strategy they use is a special type of evolutionary stable strategy. Moreover, for a single or multiple hives we prove that an ideal free distribution is achieved, and that the allocation strategy is globally optimal. In the companion paper [1] we illustrate the practical utility of these results via a multizone temperature control experiment. {\textcopyright} 2007 IEEE.},
