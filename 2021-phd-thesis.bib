doi = {10.1007/s10458-006-9004-3},
eprint = {arXiv:1311.0801v1},
file = {:home/jharwell/Downloads/Hogg2007{\_}Article{\_}CoordinatingMicroscopicRobotsI.pdf:pdf},
isbn = {1045800690043},
issn = {13872532},
keywords = {1 microscopic robots,capabilities through their ability,could provide many novel,in microscopic environments,micro-,multiagent robot control design,nanomedicine,nanorobots,nanotechnology,robots with sizes comparable,to bacteria,to sense and act},
number = {2},
pages = {427--433},
title = {{Radiative and non‐radiative transitions of electrons in local centers involving hot phonons}},
volume = {67},
year = {1975}
